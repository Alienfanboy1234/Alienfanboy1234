--Script
getgenv().SizeValue = 0.1; --Enter your speed amount here
local Player = game:service'Players'.LocalPlayer;
Player.Character.Humanoid:GetPropertyChangedSignal'Size':Connect(function()
Player.Character.Humanoid.Size = getgenv().SizeValue;
end)
Player.Character.Humanoid.Size = getgenv().SizeValue;

